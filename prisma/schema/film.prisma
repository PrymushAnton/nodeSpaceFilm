model Film {
  id             Int              @id @default(autoincrement())
  name           String           @unique
  src            String
  rating         Int
  year           Int
  baseLanguage   String
  homeCountry    String
  ageRestriction String
  description    String
  genres         GenresOnFilms[]
  photo1         String
  photo2         String
  photo3         String
  photo4         String
  actorsOnFilms  actorsOnFilms[]
  reviewsOnFilms reviewsOnFilms[]
}
