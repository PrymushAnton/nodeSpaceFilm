model Film {
  id             Int             @id @default(autoincrement())
  name           String          @unique
  src            String
  rating         Int
  year           Int
  baseLanguage   String
  homeCountry    String
  ageRestriction String
  description    String
  genres         GenresOnFilms[]
  photo1         String          @default("What")
  photo2         String          @default("What")
  photo3         String          @default("What")
  photo4         String          @default("What")
  actors         ActorsOnFilms[]
  reviews        Review[]
}
